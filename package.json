{
	"name": "gnls",
	"displayName": "GN Language Server",
	"version": "0.0.1",
	"private": false,
	"description": "Code IntelliSense for GN build system.",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/Gnls#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/Gnls/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/Gnls.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./build/index.js",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"build": "ts-node script build",
		"debug": "ts-node script debug",
		"prepare": "ts-node script prepare",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"grammars": [
			{
				"language": "gn",
				"path": "./build/grammar.json",
				"scopeName": "source.gn"
			}
		],
		"languages": [
			{
				"aliases": [
					"GN",
					"gn"
				],
				"configuration": "./build/language.json",
				"extensions": [
					"gn",
					"gni"
				],
				"id": "gn"
			}
		]
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/node": "^20.8.10",
		"node-addon-api": "^7.0.0",
		"node-gyp": "^10.0.0",
		"tslib": "^2.6.2"
	},
	"publishConfig": {
		"access": "public"
	},
	"extensionKind": [
		"workspace"
	]
}
